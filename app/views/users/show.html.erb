<h1>Manage authorized users - Show an existing user information</h1>

<div id="commands">
<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
</div>

<p>
  <b>Name:</b>
  <%=h @user.name %>
</p>

<p>
  <b>Hashed password:</b>
  <%=h @user.hashed_password %>
</p>

<p>
  <b>Salt:</b>
  <%=h @user.salt %>
</p>

<p>
  <b>Level:</b>
  <%=h @user.level %>
</p>


